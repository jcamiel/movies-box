{
  "name": "movies-box",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && npx tsc && npm run copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles --up 1 src/public/**/* src/bin/* src/views/**/* src/views/* dist",
    "start": "node dist/bin/www",
    "test": "jest",
    "fmt": "npx prettier --write src/",
    "lint": "npx eslint src/"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "csurf": "^1.11.0",
    "debug": "~4.3.4",
    "express": "~4.18.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "hbs": "^4.2.0",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/hbs": "^4.0.1",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.22.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  }
}
